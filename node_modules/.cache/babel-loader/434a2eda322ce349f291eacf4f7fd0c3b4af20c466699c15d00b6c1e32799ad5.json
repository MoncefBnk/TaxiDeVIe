\r\n\t\t<p v-if=\"passwordsDoNotMatch\" class=\"error-message\">Passwords do not match!</p>\r\n\t\t</form>\r\n  \r\n\t\t<form class=\"login\" @submit.prevent=\"login\">\r\n\t\t<h2>Login</h2>\r\n\t\t<div class=\"form-group\">\r\n\t\t<input type=\"email\" placeholder=\"Email address\" v-model=\"login_form.email\" />\r\n\t\t</div>\r\n\t\t<div class=\"form-group\">\r\n\t\t<input type=\"password\" placeholder=\"Password\" v-model=\"login_form.password\" />\r\n\t\t</div>\r\n\t\t<div class=\"form-group\">\r\n\t\t<router-link to=\"/forgot-password\" class=\"forgot-password-link\">Forgot Password?</router-link>\r\n\t\t</div>\r\n\t\t<div class=\"form-group\">\r\n\t\t<input type=\"submit\" value=\"Login\" />\r\n\t\t</div>\r\n\t\t</form>\r\n\t</section>\r\n\t</main>\r\n</template>\r\n\r\n<script>\r\nimport { ref } from 'vue';\r\nimport { useStore } from 'vuex';\r\n\r\nexport default {\r\n  setup() {\r\n    const login_form = ref({});\r\n    const register_form = ref({});\r\n    const store = useStore();\r\n    const passwordsDoNotMatch = ref(false);\r\n\r\n    const login = () => {\r\n      store.dispatch('login', login_form.value);\r\n    };\r\n\r\n    const register = () => {\r\n      if (register_form.value.password === register_form.value.confirmPassword) {\r\n        passwordsDoNotMatch.value = false;\r\n        store.dispatch('register', register_form.value);\r\n      } else {\r\n        passwordsDoNotMatch.value = true;\r\n      }\r\n    };\r\n\r\n    return {\r\n      login_form,\r\n      register_form,\r\n      login,\r\n      register,\r\n      passwordsDoNotMatch,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n  .forms {\r\n    display: flex;\r\n    min-height: 100vh;\r\n  }\r\n\r\n  form {\r\n    flex: 1 1 0%;\r\n    padding: 4rem 2rem 2rem;\r\n  }\r\n\r\n  .form-group {\r\n    margin-bottom: 2rem;\r\n  }\r\n\r\n  form.register {\r\n    color: #FFF;\r\n    background-color: rgb(245, 66, 101);\r\n    background-image: linear-gradient(to bottom right,\r\n    rgb(245, 66, 101) 0%,\r\n    rgb(189, 28, 60) 100%);\r\n    border-radius: 1rem;\r\n    padding: 2rem;\r\n    box-shadow: 0 0.5rem 1rem rgba(0, 0, 0, 0.25);\r\n  }\r\n\r\n  h2 {\r\n    font-size: 2rem;\r\n    text-transform: uppercase;\r\n    margin-bottom: 2rem;\r\n    text-align: center;\r\n  }\r\n\r\n  input {\r\n    appearance: none;\r\n    border: none;\r\n    outline: none;\r\n    background: none;\r\n    display: block;\r\n    width: 100%;\r\n    max-width: 400px;\r\n    margin: 0 auto;\r\n    font-size: 1.5rem;\r\n    margin-bottom: 1rem;\r\n    padding: 1rem 0rem;\r\n    border-radius: 0.5rem;\r\n    background-color: rgba(255, 255, 255, 0.8);\r\n    transition: background-color 0.4s;\r\n  }\r\n\r\n  input:focus {\r\n    background-color: rgba(255, 255, 255, 1);\r\n  }\r\n\r\n  input::placeholder {\r\n    color: inherit;\r\n  }\r\n\r\n  form.register input {\r\n    color: #FFF;\r\n    border: 2px solid #FFF;\r\n    background-color: transparent;\r\n  }\r\n\r\n  form.login input[type=\"submit\"],\r\n  form.register input[type=\"submit\"] {\r\n    background-color: rgb(245, 66, 101);\r\n    color: #FFF;\r\n    font-weight: 700;\r\n    padding: 1rem 2rem;\r\n    border-radius: 0.5rem;\r\n    cursor: pointer;\r\n    text-transform: uppercase;\r\n    transition: background-color 0.3s;\r\n  }\r\n\r\n  form.login input[type=\"submit\"]:hover,\r\n  form.register input[type=\"submit\"]:hover {\r\n    background-color: rgb(189, 28, 60);\r\n  }\r\n\r\n  .forgot-password-link {\r\n    color: #3498db;\r\n    text-decoration: underline;\r\n    cursor: pointer;\r\n    display: block;\r\n    text-align: center;\r\n  }\r\n\r\n  .error-message {\r\n    color: #c0392b;\r\n    margin-top: 1rem;\r\n  }\r\n</style>\r\n"],"mappings":"AA2CA,SAASA,GAAE,QAAS,KAAK;AACzB,SAASC,QAAO,QAAS,MAAM;AAE/B,eAAe;EACbC,KAAK,WAAAA,MAAA,EAAG;IACN,IAAMC,UAAS,GAAIH,GAAG,CAAC,CAAC,CAAC,CAAC;IAC1B,IAAMI,aAAY,GAAIJ,GAAG,CAAC,CAAC,CAAC,CAAC;IAC7B,IAAMK,KAAI,GAAIJ,QAAQ,CAAC,CAAC;IACxB,IAAMK,mBAAkB,GAAIN,G