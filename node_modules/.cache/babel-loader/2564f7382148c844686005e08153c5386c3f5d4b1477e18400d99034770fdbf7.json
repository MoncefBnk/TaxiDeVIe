{"ast":null,"code":"import { ref } from 'vue';\nimport { useStore } from 'vuex';\nexport default {\n  setup: function setup() {\n    var login_form = ref({});\n    var register_form = ref({});\n    var store = useStore();\n    var passwordsDoNotMatch = ref(false);\n    var login = function login() {\n      store.dispatch('login', login_form.value);\n    };\n    var register = function register() {\n      if (register_form.value.password === register_form.value.confirmPassword) {\n        passwordsDoNotMatch.value = false;\n        store.dispatch('register', register_form.value);\n      } else {\n        passwordsDoNotMatch.value = true;\n      }\n    };\n    var signInBtn = function signInBtn() {\n      document.querySelector('.container').classList.remove('right-panel-active');\n    };\n    var signUpBtn = function signUpBtn() {\n      document.querySelector('.container').classList.add('right-panel-active');\n    };\n    var registerForm = ref(null);\n    var loginForm = ref(null);\n    var toggleForms = function toggleForms() {\n      signInBtn();\n      signUpBtn();\n      registerForm.value.reset();\n      loginForm.value.reset();\n    };\n    return {\n      login_form: login_form,\n      register_form: register_form,\n      login: login,\n      register: register,\n      passwordsDoNotMatch: passwordsDoNotMatch,\n      toggleForms: toggleForms,\n      registerForm: registerForm,\n      loginForm: loginForm\n    };\n  }\n};","map":{"version":3,"names":["ref","useStore","setup","login_form","register_form","store","passwordsDoNotMatch","login","dispatch","value","register","password","confirmPassword","signInBtn","document","querySelector","classList","remove","signUpBtn","add","registerForm","loginForm","toggleForms","reset"],"sources":["C:\\Users\\Clement\\Desktop\\ProjetS4\\taxi-de-vie\\src\\views\\LoginView.vue"],"sourcesContent":["<template>\r\n\t<main class=\"login\">\r\n\t<section class=\"forms\">\r\n\t<form class=\"register\" @submit.prevent=\"register\">\r\n\t\t<h2>Register</h2>\r\n\t\t<div class=\"form-group\">\r\n\t\t<input type=\"email\" placeholder=\"Email address\" v-model=\"register_form.email\" />\r\n\t\t</div>\r\n\t\t<div class=\"form-group\">\r\n\t\t<input type=\"password\" placeholder=\"Password\" v-model=\"register_form.password\" />\r\n\t\t</div>\r\n\t\t<div class=\"form-group\">\r\n\t\t<input type=\"password\" placeholder=\"Confirm Password\" v-model=\"register_form.confirmPassword\" />\r\n\t\t</div>\r\n\t\t<div class=\"form-group\">\r\n\t\t<input type=\"phoneNumber\" placeholder=\"Phone Number \" v-model=\"register_form.phoneNumber\" />\r\n\t\t</div>\r\n\t\t<div class=\"form-group\">\r\n\t\t<input type=\"submit\" value=\"Register\" />\r\n\t\t</div>\r\n\t\t<p v-if=\"passwordsDoNotMatch\" class=\"error-message\">Passwords do not match!</p>\r\n\t\t</form>\r\n  \r\n\t\t<form class=\"login\" @submit.prevent=\"login\">\r\n\t\t<h2>Login</h2>\r\n\t\t<div class=\"form-group\">\r\n\t\t<input type=\"email\" placeholder=\"Email address\" v-model=\"login_form.email\" />\r\n\t\t</div>\r\n\t\t<div class=\"form-group\">\r\n\t\t<input type=\"password\" placeholder=\"Password\" v-model=\"login_form.password\" />\r\n\t\t</div>\r\n\t\t<div class=\"form-group\">\r\n\t\t<router-link to=\"/forgot-password\" class=\"forgot-password-link\">Forgot Password?</router-link>\r\n\t\t</div>\r\n\t\t<div class=\"form-group\">\r\n\t\t<input type=\"submit\" value=\"Login\" />\r\n\t\t</div>\r\n\t\t</form>\r\n\t</section>\r\n\t</main>\r\n</template>\r\n\r\n<script>\r\nimport { ref } from 'vue';\r\nimport { useStore } from 'vuex';\r\n\r\nexport default {\r\n  setup() {\r\n    const login_form = ref({});\r\n    const register_form = ref({});\r\n    const store = useStore();\r\n    const passwordsDoNotMatch = ref(false);\r\n\r\n    const login = () => {\r\n      store.dispatch('login', login_form.value);\r\n    };\r\n\r\n    const register = () => {\r\n      if (register_form.value.password === register_form.value.confirmPassword) {\r\n        passwordsDoNotMatch.value = false;\r\n        store.dispatch('register', register_form.value);\r\n      } else {\r\n        passwordsDoNotMatch.value = true;\r\n      }\r\n    };\r\n\r\n    const signInBtn = () => {\r\n      document.querySelector('.container').classList.remove('right-panel-active');\r\n    };\r\n\r\n    const signUpBtn = () => {\r\n      document.querySelector('.container').classList.add('right-panel-active');\r\n    };\r\n\r\n    const registerForm = ref(null);\r\n    const loginForm = ref(null);\r\n\r\n    const toggleForms = () => {\r\n      signInBtn();\r\n      signUpBtn();\r\n      registerForm.value.reset();\r\n      loginForm.value.reset();\r\n    };\r\n\r\n    return {\r\n      login_form,\r\n      register_form,\r\n      login,\r\n      register,\r\n      passwordsDoNotMatch,\r\n      toggleForms,\r\n      registerForm,\r\n      loginForm,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n  :root {\r\n    /* COLORS */\r\n    --white: #e9e9e9;\r\n    --nightblue: #001f3f;\r\n    --lightblue: #008997;\r\n    --blue: #0367a6;\r\n\r\n    /* RADII */\r\n    --button-radius: 0.7rem;\r\n\r\n    /* SIZES */\r\n    --max-width: 758px;\r\n    --max-height: 420px;\r\n\r\n    font-size: 16px;\r\n    font-family: -apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, Oxygen,\r\n      Ubuntu, Cantarell, \"Open Sans\", \"Helvetica Neue\", sans-serif;\r\n  }\r\n\r\n  body {\r\n    align-items: center;\r\n    background-color: var(--nightblue);\r\n    background: url(\"https://res.cloudinary.com/dci1eujqw/image/upload/v1616769558/Codepen/waldemar-brandt-aThdSdgx0YM-unsplash_cnq4sb.jpg\");\r\n    background-attachment: fixed;\r\n    background-position: center;\r\n    background-repeat: no-repeat;\r\n    background-size: cover;\r\n    display: grid;\r\n    height: 100vh;\r\n    place-items: center;\r\n    margin: 0; /* Remove default margin to fix unwanted space */\r\n  }\r\n\r\n  .container {\r\n    background-color: var(--nightblue);\r\n    border-radius: var(--button-radius);\r\n    box-shadow: 0 0.9rem 1.7rem rgba(0, 0, 0, 0.25),\r\n      0 0.7rem 0.7rem rgba(0, 0, 0, 0.22);\r\n    height: var(--max-height);\r\n    max-width: var(--max-width);\r\n    overflow: hidden;\r\n    position: relative;\r\n    width: 100%;\r\n    display: grid;\r\n    grid-template-columns: 1fr 1fr; /* Split into two columns for forms */\r\n  }\r\n\r\n  .container__form {\r\n    height: 100%;\r\n    transition: transform 0.6s ease-in-out;\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    justify-content: center;\r\n  }\r\n\r\n  .container--signin {\r\n    transform: translateX(0);\r\n  }\r\n\r\n  .container--signup {\r\n    transform: translateX(100%);\r\n  }\r\n\r\n  .container.right-panel-active .container--signin {\r\n    transform: translateX(-100%);\r\n  }\r\n\r\n  .container.right-panel-active .container--signup {\r\n    transform: translateX(0);\r\n  }\r\n\r\n  .overlay {\r\n    background-color: var(--lightblue);\r\n    background: url(\"https://res.cloudinary.com/dci1eujqw/image/upload/v1616769558/Codepen/waldemar-brandt-aThdSdgx0YM-unsplash_cnq4sb.jpg\");\r\n    background-attachment: fixed;\r\n    background-position: center;\r\n    background-repeat: no-repeat;\r\n    background-size: cover;\r\n    height: 100%;\r\n    position: absolute;\r\n    top: 0;\r\n    transition: transform 0.6s ease-in-out;\r\n    width: 200%;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n  }\r\n\r\n  .container.right-panel-active .overlay {\r\n    transform: translateX(-50%);\r\n  }\r\n\r\n  .overlay__panel {\r\n    text-align: center;\r\n    color: #fff;\r\n  }\r\n\r\n  .btn {\r\n    background-color: var(--blue);\r\n    background-image: linear-gradient(90deg, var(--blue) 0%, var(--lightblue) 74%);\r\n    border-radius: 20px;\r\n    border: 1px solid var(--blue);\r\n    color: var(--white);\r\n    cursor: pointer;\r\n    font-size: 0.8rem;\r\n    font-weight: bold;\r\n    letter-spacing: 0.1rem;\r\n    padding: 0.9rem 4rem;\r\n    text-transform: uppercase;\r\n    transition: transform 80ms ease-in;\r\n    margin-top: 1.5rem;\r\n  }\r\n\r\n  .btn:active {\r\n    transform: scale(0.95);\r\n  }\r\n\r\n  .btn:focus {\r\n    outline: none;\r\n  }\r\n\r\n  .form {\r\n    background-color: var(--nightblue);\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n    flex-direction: column;\r\n    padding: 0 3rem;\r\n    height: 100%;\r\n    text-align: center;\r\n  }\r\n\r\n  .input {\r\n    background-color: #fff;\r\n    border: none;\r\n    padding: 0.9rem 0.9rem;\r\n    margin: 0.5rem 0;\r\n    width: 100%;\r\n  }\r\n\r\n  @keyframes show {\r\n    0%,\r\n    49.99% {\r\n      opacity: 0;\r\n      z-index: 1;\r\n    }\r\n\r\n    50%,\r\n    100% {\r\n      opacity: 1;\r\n      z-index: 5;\r\n    }\r\n  }\r\n</style>"],"mappings":"AA2CA,SAASA,GAAE,QAAS,KAAK;AACzB,SAASC,QAAO,QAAS,MAAM;AAE/B,eAAe;EACbC,KAAK,WAAAA,MAAA,EAAG;IACN,IAAMC,UAAS,GAAIH,GAAG,CAAC,CAAC,CAAC,CAAC;IAC1B,IAAMI,aAAY,GAAIJ,GAAG,CAAC,CAAC,CAAC,CAAC;IAC7B,IAAMK,KAAI,GAAIJ,QAAQ,CAAC,CAAC;IACxB,IAAMK,mBAAkB,GAAIN,GAAG,CAAC,KAAK,CAAC;IAEtC,IAAMO,KAAI,GAAI,SAARA,KAAIA,CAAA,EAAU;MAClBF,KAAK,CAACG,QAAQ,CAAC,OAAO,EAAEL,UAAU,CAACM,KAAK,CAAC;IAC3C,CAAC;IAED,IAAMC,QAAO,GAAI,SAAXA,QAAOA,CAAA,EAAU;MACrB,IAAIN,aAAa,CAACK,KAAK,CAACE,QAAO,KAAMP,aAAa,CAACK,KAAK,CAACG,eAAe,EAAE;QACxEN,mBAAmB,CAACG,KAAI,GAAI,KAAK;QACjCJ,KAAK,CAACG,QAAQ,CAAC,UAAU,EAAEJ,aAAa,CAACK,KAAK,CAAC;MACjD,OAAO;QACLH,mBAAmB,CAACG,KAAI,GAAI,IAAI;MAClC;IACF,CAAC;IAED,IAAMI,SAAQ,GAAI,SAAZA,SAAQA,CAAA,EAAU;MACtBC,QAAQ,CAACC,aAAa,CAAC,YAAY,CAAC,CAACC,SAAS,CAACC,MAAM,CAAC,oBAAoB,CAAC;IAC7E,CAAC;IAED,IAAMC,SAAQ,GAAI,SAAZA,SAAQA,CAAA,EAAU;MACtBJ,QAAQ,CAACC,aAAa,CAAC,YAAY,CAAC,CAACC,SAAS,CAACG,GAAG,CAAC,oBAAoB,CAAC;IAC1E,CAAC;IAED,IAAMC,YAAW,GAAIpB,GAAG,CAAC,IAAI,CAAC;IAC9B,IAAMqB,SAAQ,GAAIrB,GAAG,CAAC,IAAI,CAAC;IAE3B,IAAMsB,WAAU,GAAI,SAAdA,WAAUA,CAAA,EAAU;MACxBT,SAAS,CAAC,CAAC;MACXK,SAAS,