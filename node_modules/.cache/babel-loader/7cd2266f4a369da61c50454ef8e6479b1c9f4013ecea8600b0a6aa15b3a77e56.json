{"ast":null,"code":"import { ref, onMounted, onUnmounted } from 'vue';\nimport { useStore } from 'vuex';\nexport default {\n  setup: function setup() {\n    var login_form = ref({});\n    var register_form = ref({});\n    var store = useStore();\n    var passwordsDoNotMatch = ref(false);\n    var isMobile = ref(window.innerWidth <= 768);\n    var login = function login() {\n      store.dispatch('login', login_form.value);\n    };\n    var register = function register() {\n      if (register_form.value.password === register_form.value.confirmPassword) {\n        passwordsDoNotMatch.value = false;\n        store.dispatch('register', register_form.value);\n      } else {\n        passwordsDoNotMatch.value = true;\n      }\n    };\n\n    // Update isMobile on window resize\n    var updateWindowSize = function updateWindowSize() {\n      isMobile.value = window.innerWidth <= 768;\n    };\n\n    // Add event listeners\n    onMounted(function () {\n      window.addEventListener('resize', updateWindowSize);\n    });\n\n    // Remove event listeners on component unmount\n    onUnmounted(function () {\n      window.removeEventListener('resize', updateWindowSize);\n    });\n    return {\n      login_form: login_form,\n      register_form: register_form,\n      login: login,\n      register: register,\n      passwordsDoNotMatch: passwordsDoNotMatch,\n      isMobile: isMobile\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","onUnmounted","useStore","setup","login_form","register_form","store","passwordsDoNotMatch","isMobile","window","innerWidth","login","dispatch","value","register","password","confirmPassword","updateWindowSize","addEventListener","removeEventListener"],"sources":["C:\\Users\\Clement\\Desktop\\ProjetS4\\taxi-de-vie\\src\\components\\RegisterPage.vue"],"sourcesContent":["<template>\r\n  <main class=\"login\">\r\n    <section class=\"forms\">\r\n      <!-- Register Form -->\r\n      <form class=\"register\" @submit.prevent=\"register\" v-show=\"!isMobile\">\r\n        <h2>Welcome to Taxi de vie</h2>\r\n        <p v-if=\"passwordsDoNotMatch\">Passwords do not match!</p>\r\n        <!-- Your form input fields go here -->\r\n      </form>\r\n\r\n      <!-- Login Form -->\r\n      <form class=\"login\" @submit.prevent=\"login\">\r\n        <h2>Login</h2>\r\n        <input type=\"email\" placeholder=\"Email address\" v-model=\"login_form.email\" />\r\n        <input type=\"password\" placeholder=\"Password\" v-model=\"login_form.password\" />\r\n        <router-link to=\"/forgot-password\" class=\"forgot-password-link\">Forgot Password?</router-link>\r\n        <input type=\"submit\" value=\"Login\" />\r\n        <p>Don't have an account yet ?</p>\r\n        <router-link to=\"/register\" class=\"register\">Create an account now !</router-link>\r\n      </form>\r\n    </section>\r\n  </main>\r\n</template>\r\n\r\n<script>\r\nimport { ref, onMounted, onUnmounted } from 'vue';\r\nimport { useStore } from 'vuex';\r\n\r\nexport default {\r\n  setup() {\r\n    const login_form = ref({});\r\n    const register_form = ref({});\r\n    const store = useStore();\r\n    const passwordsDoNotMatch = ref(false);\r\n    const isMobile = ref(window.innerWidth <= 768);\r\n\r\n    const login = () => {\r\n      store.dispatch('login', login_form.value);\r\n    };\r\n\r\n    const register = () => {\r\n      if (register_form.value.password === register_form.value.confirmPassword) {\r\n        passwordsDoNotMatch.value = false;\r\n        store.dispatch('register', register_form.value);\r\n      } else {\r\n        passwordsDoNotMatch.value = true;\r\n      }\r\n    };\r\n\r\n    // Update isMobile on window resize\r\n    const updateWindowSize = () => {\r\n      isMobile.value = window.innerWidth <= 768;\r\n    };\r\n\r\n    // Add event listeners\r\n    onMounted(() => {\r\n      window.addEventListener('resize', updateWindowSize);\r\n    });\r\n\r\n    // Remove event listeners on component unmount\r\n    onUnmounted(() => {\r\n      window.removeEventListener('resize', updateWindowSize);\r\n    });\r\n\r\n    return {\r\n      login_form,\r\n      register_form,\r\n      login,\r\n      register,\r\n      passwordsDoNotMatch,\r\n      isMobile,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.forms {\r\n  display: flex;\r\n  min-height: 100vh;\r\n}\r\n\r\nform {\r\n  flex: 1 1 0%;\r\n  padding: 8rem 1rem 1rem;\r\n}\r\n\r\n@media screen and (max-width: 768px) {\r\n  .forms {\r\n    flex-direction: column;\r\n  }\r\n\r\n  form {\r\n    padding: 2rem 1rem 1rem;\r\n  }\r\n\r\n  /* Hide the entire form.register section in mobile format */\r\n  form.register {\r\n    display: none;\r\n  }\r\n}\r\n\r\nform.register {\r\n  color: #fff;\r\n  background-color: rgb(245, 66, 101);\r\n  background-image: linear-gradient(to bottom right, rgb(245, 66, 101) 0%, rgb(189, 28, 60) 100%);\r\n}\r\n\r\nh2 {\r\n  font-size: 2rem;\r\n  text-transform: uppercase;\r\n  margin-bottom: 2rem;\r\n}\r\n\r\ninput {\r\n  appearance: none;\r\n  border: none;\r\n  outline: none;\r\n  background: none;\r\n  display: block;\r\n  width: 100%;\r\n  max-width: 400px;\r\n  margin: 0 auto;\r\n  font-size: 1.5rem;\r\n  margin-bottom: 2rem;\r\n  padding: 0.5rem 0rem;\r\n}\r\n\r\ninput:not([type=\"submit\"]) {\r\n  opacity: 0.8;\r\n  transition: 0.4s;\r\n}\r\n\r\ninput:focus:not([type=\"submit\"]) {\r\n  opacity: 1;\r\n}\r\n\r\ninput::placeholder {\r\n  color: inherit;\r\n}\r\n\r\nform.register input:not([type=\"submit\"]) {\r\n  color: #fff;\r\n  border-bottom: 2px solid #fff;\r\n}\r\n\r\nform.login input:not([type=\"submit\"]) {\r\n  color: #2c3e50;\r\n  border-bottom: 2px solid #2c3e50;\r\n}\r\n\r\nform.login input[type=\"submit\"] {\r\n  background-color: rgb(245, 66, 101);\r\n  color: #fff;\r\n  font-weight: 700;\r\n  padding: 1rem 2rem;\r\n  border-radius: 0.5rem;\r\n  cursor: pointer;\r\n  text-transform: uppercase;\r\n}\r\n\r\nform.register input[type=\"submit\"] {\r\n  background-color: #fff;\r\n  color: rgb(245, 66, 